[Encoder]
	Name=gcn-full
	RegularizationParameter=0.0
	NumberOfConvolutions=2

[Decoder]
	Name=distmult

[Shared]
	EmbeddingWidth=10

[Optimizer]
	Minibatches=No
	MaxGradientNorm=1
	ReportTrainLossEvery=100

	[EarlyStopping]
		CheckEvery=500
		BurninPhaseDuration=5000

	[Algorithm]
		Name=Adam
		learning_rate=0.01

[General]
	NegativeSampleRate=10
	Backend=theano

