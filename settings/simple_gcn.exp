[Encoder]
	Name=gcn
	DropoutKeepProbability=0.8
	InternalEncoderDimension=30

[Decoder]
	Name=bilinear-diag
	RegularizationParameter=0.01

[Shared]
	CodeDimension=100

[Optimizer]
	Minibatches=No
	MaxGradientNorm=1
	ReportTrainLossEvery=20

	[EarlyStopping]
		CheckEvery=1000
		BurninPhaseDuration=200

	[Algorithm]
		Name=Adam
		learning_rate=0.01

[General]
	NegativeSampleRate=1