[Encoder]
	Name=embedding

[Decoder]
	Name=bilinear-diag
	RegularizationParameter=0.01

[Shared]
	EmbeddingWidth=200

[Optimizer]
	Minibatches=Yes
	MaxGradientNorm=1
	ReportTrainLossEvery=50
	BatchSize=5000

	[EarlyStopping]
		CheckEvery=500
		BurninPhaseDuration=50000

	[Algorithm]
		Name=Adam
		learning_rate=0.001

[General]
	NegativeSampleRate=4