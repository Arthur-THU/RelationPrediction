[Encoder]
	Name=embedding

[Decoder]
	Name=bilinear-diag
	RegularizationParameter=0.01

[Shared]
	CodeDimension=100

[Optimizer]
	MaxGradientNorm=1
	ReportTrainLossEvery=5
	BatchSize=5000
	Minibatches=Yes

	[EarlyStopping]
		CheckEvery=10

	[Algorithm]
		Name=Adam
		learning_rate=0.01

[General]
	NegativeSampleRate=10
	GraphSplitSize=15000


[Evaluation]
	Metric=MRR