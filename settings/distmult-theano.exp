[Encoder]
	Name=embedding
	RegularizationParameter=0.01

[Decoder]
	Name=distmult

[Shared]
	EmbeddingWidth=200

[Optimizer]
	Minibatches=Yes
	MaxGradientNorm=1
	ReportTrainLossEvery=500	
	BatchSize=1000

	[EarlyStopping]
		CheckEvery=5000
		BurninPhaseDuration=2000

	[Algorithm]
		Name=Adam
		learning_rate=0.01

[General]
	NegativeSampleRate=10
	Backend=theano

