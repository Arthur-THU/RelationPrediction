[Encoder]
	Name=gcn-intermediate-full
	RegularizationParameter=0.1
	NumberOfConvolutions = 4

[Decoder]
	Name=distmult

[Shared]
	EmbeddingWidth=200

[Optimizer]
	Minibatches=No
	MaxGradientNorm=1
	ReportTrainLossEvery=5	

	[EarlyStopping]
		CheckEvery=100
		BurninPhaseDuration=2000

	[Algorithm]
		Name=Adam
		learning_rate=0.0001

[General]
	NegativeSampleRate=10
	Backend=tensorflow

