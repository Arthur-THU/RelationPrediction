[Encoder]
	Name=embedding
	RegularizationParameter=0.01

[Decoder]
	Name=distmult_with_transform

[Shared]
	EmbeddingWidth=200

[Optimizer]
	Minibatches=Yes
	MaxGradientNorm=1
	ReportTrainLossEvery=100	
	BatchSize=5000

	[EarlyStopping]
		CheckEvery=2000
		BurninPhaseDuration=2000

	[Algorithm]
		Name=Adam
		learning_rate=0.002

[General]
	NegativeSampleRate=10
	Backend=tensorflow

