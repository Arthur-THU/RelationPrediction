[Encoder]
	Name=simple_gated_basis_gcn
	DropoutKeepProbability=0.8

[Decoder]
	Name=nonlinear-transform
	RegularizationParameter=0.01
	DecoderDimension=100

[Shared]
	EmbeddingWidth=30

[Optimizer]
	Minibatches=No
	MaxGradientNorm=1
	ReportTrainLossEvery=20

	[EarlyStopping]
		CheckEvery=1000
		BurninPhaseDuration=200

	[Algorithm]
		Name=Adam
		learning_rate=0.1

[General]
	NegativeSampleRate=1