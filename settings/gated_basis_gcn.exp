[Encoder]
	Name=gated_basis_gcn
	DropoutKeepProbability=0.999

[Decoder]
	Name=bilinear-diag
	RegularizationParameter=0.01

[Shared]
	EmbeddingWidth=100

[Optimizer]
	Minibatches=No
	MaxGradientNorm=1
	ReportTrainLossEvery=20

	[EarlyStopping]
		CheckEvery=1000
		BurninPhaseDuration=200

	[Algorithm]
		Name=Adam
		learning_rate=0.001

[General]
	NegativeSampleRate=1