def read_triplets(filename):
    for line in open(filename, 'r+'):
        processed_line = line.strip().split('\t')
        yield processed_line

def read_triplet_file(filename):
    return list(read_triplets(filename))


def enumerate_entities(triplet_list):
    entity_set = set([])

    for triplet in triplet_list:
        entity_set.add(triplet[0])
        entity_set.add(triplet[2])

    return list(enumerate(entity_set))    

def enumerate_relations(triplet_list):
    relation_set = set([])

    for triplet in triplet_list:
        relation_set.add(triplet[1])

    return list(enumerate(relation_set))


triplets = read_triplet_file('freebase_mtr100_mte100-train.txt')

print(enumerate_relations(triplets))
